    <style>
        body {
            text-align: center;
        }

        div#editor {
            width: 81%;
            margin: auto;
            text-align: left;
        }
    </style>






    <style>
        /* width */
        ::-webkit-scrollbar {
            width: 10px;
        }

        /* Track */
        ::-webkit-scrollbar-track {
            box-shadow: inset 0 0 5px #7BC042;
            border-radius: 10px;
            width: 10px;
        }

        /* Handle */
        ::-webkit-scrollbar-thumb {
            background: #7BC042;
            border-radius: 10px;
            width: 10px;
        }

            /* Handle on hover */
            ::-webkit-scrollbar-thumb:hover {
                background: #7BC042;
                width: 10px;
            }
    </style>
    <style>
        .nav-tog {
            color: #ddd !important;
        }

            .nav-tog:hover {
                color: #ddd !important;
            }

            .nav-tog:focus {
                color: #ddd !important;
            }

            .nav-tog:focus {
                color: #ddd !important;
            }

        .nav-link {
            color: white !important;
            transition: font-size .5s !important;
        }

            .nav-link .nav-icon {
                color: wheat !important;
            }

            .nav-link:hover .nav-icon {
                color: black !important;
                font-size: 16px !important;
            }

        .nav-item .active {
            background-color: #007bff !important;
        }

        .nav-link .active {
            background-color: #007bff !important;
        }

        .nav-link:hover {
            font-size: 13px !important;
        }

        .navbar-tog:hover {
            color: white !important;
        }

        @@media (min-width: 992px) {
            .ForResponsive {
                display: none
            }
        }
    </style>
    <environment include="Development">
        <!-- VENDOR CSS -->
        <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" asp-append-version="true" />
